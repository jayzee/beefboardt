<div class="center jumbotron specialjum">
  <div class="over container body-content center">
        <h1>Create an Event!</h1>
      <%= form_tag(new_event_path, method: 'GET') do %>
        <p><%= label_tag "Event Name:" %>
        <%= text_field_tag :name %>
      <%= submit_tag "Let's go", class: "btn btn-beyonce-primary btn-lg", role: "button" %></p>
      <% end %>
    </div>

</div>

<br><br>


  <div class="container">

      <h2>Here's upcoming events for you to enjoy:</h2>


    
        <%= render partial: "events/snapshot", collection: @events, :as => :event %>
