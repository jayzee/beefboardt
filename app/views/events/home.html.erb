<div class="center jumbotron specialjum">
  <div class="over container body-content center text-center">
    <h1>CREATE AN EVENT</h1>
      <%= form_tag(new_event_path, method: 'GET') do %>
        <p><%= text_field_tag :name %>  <%= submit_tag "Let's go", class: "btn btn-beyonce-primary-homepage btn-lg", role: "button" %></p>
      <% end %>
  </div>
</div>

<br><br>

<div class="container">
  <div class="row homepage-events">
    <%= render partial: "events/snapshot-home", collection: @events, :as => :event %>

  </div>
</div>
